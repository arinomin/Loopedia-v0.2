// 音符の画像URLからSVGコンテンツを取得する関数
export function getNoteImageSvg(notePath: string): string | undefined {
  // デバッグ
  console.log(`getNoteImageSvg - Input path: ${notePath}`);
  
  // notesNパターンの抽出（パスやURLからでも）
  // 例: "/images/notes_svg/notes1.svg" や "notes1" から "1" を取得
  const match = notePath.match(/notes(\d+)/);
  if (!match) {
    console.warn(`[SVG] パスから音符番号を抽出できませんでした: ${notePath}`);
    return undefined;
  }
  
  const number = match[1];
  const key = `notes${number}` as keyof typeof noteSvgContent;
  
  // SVGコンテンツの確認
  if (!noteSvgContent[key]) {
    console.warn(`[SVG] 音符のSVGコンテンツがありません: Key=${key}, Path=${notePath}`);
    return undefined;
  }
  
  // SVGコンテンツを返す
  return noteSvgContent[key];
}